{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "channel": "production"
    },
    "production-aab": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "channel": "production-aab"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "$(GOOGLE_PLAY_SERVICE_ACCOUNT_KEY_PATH)",
        "track": "internal"
      }
    },
    "closed-alpha": {
      "android": {
        "serviceAccountKeyPath": "$(GOOGLE_PLAY_SERVICE_ACCOUNT_KEY_PATH)",
        "track": "alpha"
      }
    }
  }
}
